{% extends "EzPublishCoreBundle::content_fields.html.twig" %}

{% block ezcustomform_field %}
    {% if field.value.formId is not null %}
        {% set controllerAction = 'Novactive\\Bundle\\eZFormBuilderBundle\\Controller\\Front\\FormController:show' %}
        {{ render_esi(controller(controllerAction, {id: field.value.formId})) }}
    {% endif %}
{% endblock %}